{
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [".tsx"],
        "alias": {
          "_assets": "./src/assets",
          "_components": "./src/components",
          "_screens": "./src/screens",
          "_styles": "./src/styles",
          "_utils": "./src/utils"
        }
      }
    ]
  ]
}
